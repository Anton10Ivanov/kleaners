  "mcpServers": {
    "shadcn": {
      "name": "shadcn/ui MCP Server",
      "description": "MCP server for shadcn/ui component management",
      "command": "npx",
      "args": ["shadcn@latest", "mcp"],
      "version": "latest",
      "capabilities": [
        "component_management",
        "registry_access",
        "component_installation"
      ]
    }
