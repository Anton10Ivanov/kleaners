            {
              "name": "card.tsx",
              "path": "components/ui/card.tsx"
            }
          ],
          "dependencies": []
        },
        "input": {
          "name": "Input",
          "description": "Displays a form input field",
          "files": [
            {
              "name": "input.tsx",
              "path": "components/ui/input.tsx"
            }
          ],
          "dependencies": []
        },
        "label": {
          "name": "Label",
